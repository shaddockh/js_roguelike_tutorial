{
  "name": "web-boilerplate",
  "version": "0.0.1",
  "description": "Base setup for starting a new web project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaddockh/web-boilerplate.git"
  },
  "author": "T. Shaddock Heath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shaddockh/web-boilerplate/issues"
  },
  "devDependencies": {
    "browserify": "latest",
    "debowerify": "^0.8.1",
    "entity-blueprint-manager": "^0.1.0",
    "grunt": "latest",
    "grunt-browserify": "latest",
    "grunt-contrib-connect": "latest",
    "grunt-contrib-copy": "latest",
    "grunt-contrib-jshint": "latest",
    "grunt-contrib-watch": "latest",
    "grunt-jsbeautifier": "latest",
    "grunt-mocha": "latest",
    "grunt-mocha-test": "latest",
    "matchdep": "latest",
    "should": "latest",
    "sprintf-js": "0.0.7"
  },
  "dependencies": {
    "browserify-shim": "^3.8.0",
    "folderify": "^0.6.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "debowerify",
      "folderify"
    ]
  },
  "browserify-shim": {
    "jquery": "$",
    "rot": "ROT",
    "bootstrap": {
      "depends": [
        "jquery"
      ]
    }
  },
  "browser": {
    "rot": "./bower_components/rot.js/rot.js",
    "bootstrap": "./bower_components/bootstrap/dist/js/bootstrap.min.js",
    "jquery": "./bower_components/jquery/dist/jquery.js"
  }
}
